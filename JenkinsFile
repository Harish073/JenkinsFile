pipeline {
    agent any 
    stages {
        stage('build') {
            steps {
               echo 'built'
            }
        }
        stage('test ') {
            steps {
                echo 'tested' 
            }
        }
        stage('deploy') {
            steps {
                echo 'deployed' 
            }
        }
    }
    post{
        always{
            echo 'success or failure'
        }
        success{
            echo'success'
        }
        failure{
            echo'Failure'
        }
    }
}
